{
   "version": 2,
   "builds": [
     {
       "src": "/api/**/*.js",
       "use": "@vercel/node"
     }
   ],
   "routes": [
     { "src": "/static/(.*)", "dest": "/public/static/$1" },
     { "src": "/(.*)", "dest": "/build/index.html" }
   ],
   "env": {
     "OPENAI_KEY": "@openai_key"
   }
 }